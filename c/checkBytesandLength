#include <stdio.h>
#include <stdlib.h>

int main(void){
        int a[5]; // on mimi integers are 4 bytes, so array of 5 has 5*4 bytes
        printf("The array itself has size %ld\n", sizeof(a)); // prints out 20
        printf("The elements have size %ld\n", sizeof((a[0])));
        printf("Length of array is %ld\n", sizeof(a)/sizeof((a[0])));


        return 0;
        }
